{
  "name": "ory-editor-plugins-slate",
  "version": "0.0.0",
  "main": "./lib/index.js",
  "author": "ORY GmbH",
  "license": "GNU Lesser General Public License v3.0",
  "scripts": {
    "build-now": "NODE_ENV=production npm-run-all --parallel build:lib build:css",
    "build": "npm-run-all --parallel build:lib build:css",
    "build:watch": "npm-run-all --parallel build:lib:watch build:css:watch",
    "build:lib": "babel ./src --source-maps --ignore test.js --out-dir ./lib",
    "build:lib:watch": "npm run build:lib -- --watch",
    "build:css": "postcss -c ../../../../postcss.config.js --dir lib/ src/index.css",
    "build:css:watch": "npm run build:css -- -w",
    "docs": "documentation build ./src/**/*.js --format md --github -o ../../../../docs/api/plugins/slate.md",
    "prebuild": "npm run clean",
    "clean": "rimraf \"lib/*\"",
    "flow:generate": "flow-typed install  --flowVersion=0.79.1"
  },
  "peerDependencies": {
    "@material-ui/core": "^3.0.3",
    "@material-ui/icons": "^3.0.1",
    "ory-editor-core": "*",
    "ory-editor-ui": "*",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-router-dom": "*"
  },
  "dependencies": {
    "@gitbook/slate": "^0.34.10",
    "@gitbook/slate-edit-blockquote": "^0.6.1",
    "@gitbook/slate-edit-list": "^0.12.2",
    "@gitbook/slate-html-serializer": "^0.6.17-0",
    "@gitbook/slate-plain-serializer": "^0.5.25",
    "@gitbook/slate-react": "^0.13.8",
    "@gitbook/slate-schema-violations": "^0.1.23",
    "deep-rename-keys": "^0.2.1",
    "immutable": "3.8.2",
    "parse5": "^3.0.2",
    "ramda": "^0.24.1",
    "react-portal": "4.1.5",
    "redux-undo": "1.0.0-beta9-7",
    "selection-position": "^1.0.0"
  }
}
